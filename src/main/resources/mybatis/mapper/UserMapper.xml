<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.board.mapper.UserMapper">

	<select id="selectUserByEmail" resultType="com.example.board.vo.User" parameterType="com.example.board.vo.User">
		select * 
		from user 
		where emaail = #{email}
	</select>
	
	<select id="getNow" resultType="java.lang.String">
        SELECT NOW()
    </select>
</mapper>